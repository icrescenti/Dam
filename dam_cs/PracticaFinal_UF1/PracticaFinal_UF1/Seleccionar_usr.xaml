        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaFinal_UF1"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Loaded="seleccionar_Loaded"
        Title="Seleccionar Usuari" MinHeight="450" MinWidth="800" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dg_grid" HorizontalAlignment="Stretch" Height="auto" VerticalAlignment="Stretch" Width="auto" ItemsSource="{Binding}" IsReadOnly="True" CanUserResizeColumns="False" ColumnWidth="*" CanUserResizeRows="False" MouseDoubleClick="dg_grid_MouseDoubleClick" Loaded="seleccionar_Loaded" />
    </Grid>
</Window>
